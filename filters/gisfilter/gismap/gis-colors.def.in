    set radcolor(bref,background) [::nbsp::gis::x11color white];
    set radcolor(bref,0) [::nbsp::gis::x11color white];
    set radcolor(bref,5) [::nbsp::gis::x11color aqua];
    set radcolor(bref,10) [::nbsp::gis::x11color deepskyblue];
    set radcolor(bref,15) [::nbsp::gis::x11color blue];
    set radcolor(bref,20) [::nbsp::gis::x11color lawngreen];
    set radcolor(bref,25) [::nbsp::gis::x11color limegreen];
    set radcolor(bref,30) [::nbsp::gis::x11color green];
    set radcolor(bref,35) [::nbsp::gis::x11color yellow];
    set radcolor(bref,40) [::nbsp::gis::x11color gold];
    set radcolor(bref,45) [::nbsp::gis::x11color orange];
    set radcolor(bref,50) [::nbsp::gis::x11color red];
    set radcolor(bref,55) [::nbsp::gis::x11color firebrick];
    set radcolor(bref,60) [::nbsp::gis::x11color brown];
    set radcolor(bref,65) [::nbsp::gis::x11color magenta];
    set radcolor(bref,70) [::nbsp::gis::x11color darkviolet];
    set radcolor(bref,75) [::nbsp::gis::x11color white];

    # New
    @gis-colors-bref@
