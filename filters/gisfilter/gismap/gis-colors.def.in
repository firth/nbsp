namespace eval ::nbsp::gis {

    variable x11color;

    set x11color(white) {255 255 255};
    set x11color(black) {0 0 0};
    set x11color(darkviolet) {148 0 211};
    set x11color(magenta) {255 0 255};
    set x11color(brown) {165 42 42};
    set x11color(firebrick) {178 34 34};
    set x11color(red) {255 0 0};
    set x11color(orange) {255 165 0};
    set x11color(gold) {255 215 0};
    set x11color(yellow) {255 255 0};
    set x11color(green) {0 255 0};
    set x11color(limegreen) {50 205 50};
    set x11color(lawngreen) {124 252 0};
    set x11color(blue) {0 0 255};
    set x11color(deepskyblue) {0 191 255};
    set x11color(aqua) {0 255 255};
}

namespace eval ::nbsp::gis {

    variable rgbcolor;

    @gis-colors-rgb@
}

namespace eval ::nbsp::gis {

